<?xml version="1.0" encoding="UTF-8"?>
<Defs>
 
	<!-- Apiary -->
	<ProcessorFramework.ProcessDef Name="ISA_ApiaryBase" Abstract="True">
		<capacityFactor>20.0</capacityFactor>
		<efficiency>0.1</efficiency>
		<usesTemperature>true</usesTemperature>
		<temperatureSafe>5.0~40.0</temperatureSafe>
		<temperatureIdeal>10.0~37.0</temperatureIdeal>
		<ruinedPerDegreePerHour>2</ruinedPerDegreePerHour><!-- percent per degree per hour -->
		<speedBelowSafe>0.1</speedBelowSafe>
		<speedAboveSafe>1.0</speedAboveSafe>
		<sunFactor>1.0~1.0</sunFactor>
		<rainFactor>0.5~1.0</rainFactor>
		<snowFactor>0.5~1.0</snowFactor>
		<windFactor>1.0~1.0</windFactor>
		<unpoweredFactor>0.0</unpoweredFactor>
		<unfueledFactor>0.0</unfueledFactor>
		<powerUseFactor>0.0</powerUseFactor>
		<fuelUseFactor>0.0</fuelUseFactor>
		<filledGraphicSuffix>Filled</filledGraphicSuffix>
		<usesQuality>false</usesQuality>
<!--  	<qualityDays>(1, 3, 8, 14, 20, 50, 120)</qualityDays>  -->
		<color>(255, 255, 255)</color><!-- 0-255 or 0.0-1.0 -->
		<customLabel/>
		<destroyChance>0.0</destroyChance>
	</ProcessorFramework.ProcessDef>

  <ProcessorFramework.ProcessDef ParentName="ISA_ApiaryBase">
		<defName>ISA_MeatIntoHoney</defName>
		<thingDef>ISA_HornetHoney</thingDef>
	  	<processDays>2</processDays><!-- in days, a day is 60,000 ticks -->
		<ingredientFilter>
			<thingCategoryDefs>
				<li>Meat</li>
			</thingCategoryDefs>
		</ingredientFilter>
	</ProcessorFramework.ProcessDef>

  <ProcessorFramework.ProcessDef ParentName="ISA_ApiaryBase">
		<defName>ISA_MeatIntoLarvae</defName>
		<thingDef>ISA_DeathstingerLarvae</thingDef>
	  	<processDays>5</processDays><!-- in days, a day is 60,000 ticks -->
		<ingredientFilter>
			<thingCategoryDefs>
				<li>Meat</li>
			</thingCategoryDefs>
		</ingredientFilter>
	</ProcessorFramework.ProcessDef>

</Defs>
